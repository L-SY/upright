          scaling 1e+1
          (0,0) 2.0   ; input_upper_1
          (1,0) 2.0   ; input_upper_2
          (2,0) 80.0   ; input_upper_3
          (3,0) 80.0   ; input_upper_4
          (4,0) 80.0   ; input_upper_5
          (5,0) 80.0   ; input_upper_6
          (6,0) 80.0   ; input_upper_7
          (7,0) 80.0   ; input_upper_8
