<launch>
    <param name="robot_description"
           command="$(find xacro)/xacro $(find upright_asset)/description/urdf/diablo_manipulator/robot.xacro"/>

    <param name="wlm_description"
           command="$(find xacro)/xacro $(find upright_asset)/description/urdf/diablo_manipulator/robot.xacro"/>

    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find upright_asset)/worlds/empty_world.world"/>
        <arg name="paused" value="true"/>
    </include>
    <!-- push robot_description to factory and spawn robot in gazebo -->
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" clear_params="true"
          output="screen"
          args="-param wlm_description -urdf -model wlm
        -z 0.
        "
    />
</launch>